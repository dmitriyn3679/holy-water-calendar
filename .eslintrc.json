{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "import-quotes"
    ],
    "extends": [
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "object-curly-spacing": ["error", "always"],
        "import-quotes/import-quotes": [1, "single"],
        "react/prop-types": "off"
    }
}
